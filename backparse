function addCassiers(){
    Cassiers = document.getElementsByName("Simbian")[0].value;
    blockCassiers(Cassiers);
}
function clearText(){
    document.getElementsByName("Simbian")[0].value = "";
}

function SearchPNFP(PnfpInf) {
    PnfpArr = [5556, 5556, 6251, 6246, 6241, 8040, 8054, 8329, 8233, 8210, 8330, 8241, 8331, 8332, 8070, 8240, 8333, 8103, 8334, 8335, 8391, 8392, 8393, 8394, 8395];
    TwoCif = ["10", "11", "22", "16", "14", "04", "08", "15", "23", "12", "17", "26", "09", "20", "06", "25", "02", "24", "03", "18", "27", "21", "07", "13", "05"];
    PNFP = PnfpInf.replace(/\s+/g, '');
    if (PNFP.length == 5) {
        FSpnfp = PNFP.slice(0, 2);
        //Номер позиции в массиве

        function finder(array, value) {
            for (var i = 0; i < array.length; i++) {
                if (array[i] == value) return i;
            }
            return 1;
        }
        NumArray = finder(TwoCif, FSpnfp);
        if (PNFP.slice(0, 3) == "000") {
            NumArray = PNFP.slice(-2) - 1 + 1;
            if (PNFP.slice(-2) > 24) {
                //alert("Филиал "+PNFP.slice(-2)+" не существует!!!");
                //document.location.href = tooda;
                return undefined;
                //NumArray =1;//для предотвращения ошибки
            }
        }

        IdFilials = PnfpArr[NumArray];
        Host = location.host;
        var xhr = new XMLHttpRequest();
        xhr.open('GET', "http://" + Host + "/WebUI.Retail/Agencies.aspx?ID=1&pID=5555&ppID=" + IdFilials, false);
        xhr.send(null);
        var SearchFilials = xhr.responseText;
        PnfpStr = "№" + PNFP;
        if (PNFP == '00000') {
            PnfpStr = "Каса";
        }
        if (SearchFilials.indexOf(PnfpStr) != -1) {
            hrefPNFP = SearchFilials.slice(SearchFilials.indexOf('href="', SearchFilials.indexOf(PnfpStr)) + 6, SearchFilials.indexOf('Пользователи', SearchFilials.indexOf(PnfpStr)) - 2);
            adress = 'http://' + Host + '/WebUI.Retail/' + hrefPNFP;
            adress = adress.replace("amp;", "").replace("amp;", "").replace("amp;", "");
            //document.location.href = adress;
        } else {
            if (SearchFilials.indexOf("Server Error in '/WebUI.Retail' Application.") == -1 && SearchFilials.indexOf("Пожалуйста, подключитесь") == -1) {
                //alert("Указанный ПНФП не существует!");
                return undefined;
            } else {
                alert("Нужно перелогинится!");
                //document.location.href = "http://"+ Host +"/WebUI.Retail/Login.aspx";
                return null;
            }

        }
    } else {
        //alert("Количество цифр - "+PNFP.length);
        return undefined;
    }
    return adress;
}

function blockCassiers(ListCassiers) {
    var UndefinedPNFP = ""; //Пустой массив для учетных записей с ненайдеными ПНФП
   var DontFind = ""; //пустой массив для записей которых не нашли на ПНФП
    //var ErrorCassiers = "";
    //var ListCassiers = prompt("внесите данные новых кассиров"); //ввод списка кассиров
    ListCassiers = ListCassiers.replace(/\s+/g, ' ').replace(/\s*$/, ''); //удаление лишних пробелов
    CassiersAll = ListCassiers.split(' '); // розбивние на массив
    //console.log(CassiersAll.length);
    if (CassiersAll.length % 5 === 0) { //Проверка на соответстиве таблицы кассиров
        var k = CassiersAll.length / 5; // количество кассиров
        for (j = 0; j < k; j++) {
            num1 = j * 5;
            //alert(num1);
            newPnfp = CassiersAll[num1];
            adress = SearchPNFP(newPnfp);
            if (adress === null) {
                UndefinedPNFP = UndefinedPNFP + CassiersAll[num1] + "\t" + CassiersAll[num1 + 1] + " " + CassiersAll[num1 + 2] + " " + CassiersAll[num1 + 3] + "\t" + CassiersAll[num1 + 4] + "\n";
                prompt("При поиске ПНФП обнаружена ошибка", newPnfp);
                break;
            }
            if (adress !== undefined) {
                //alert(newPnfp);
                //1. Отправляем GET запрос для получения HTML старници отделения
                var xhr = new XMLHttpRequest();
                xhr.open('GET', adress, false); //adress - парметр пеердаваемый из функции SearchPNFP(PnfpInf)
                xhr.send(null);
                var FirstResponse = xhr.responseText;
                var login = CassiersAll[num1 + 4];
                if (FirstResponse.indexOf(login) !== -1) {
                    //alert(login);
                //Берем значение нужных элементов для следующего POST запроса
                var vieState = FirstResponse.slice(FirstResponse.indexOf('VIEWSTATE" value=') + 18, FirstResponse.indexOf('"', FirstResponse.indexOf('VIEWSTATE" value=') + 18));
                var vieStateGenerator = FirstResponse.slice(FirstResponse.indexOf('VIEWSTATEGENERATOR" value=') + 27, FirstResponse.indexOf('"', FirstResponse.indexOf('VIEWSTATEGENERATOR" value=') + 27));
                var eventValidation = FirstResponse.slice(FirstResponse.indexOf('EVENTVALIDATION" value=') + 24, FirstResponse.indexOf('"', FirstResponse.indexOf('EVENTVALIDATION" value=') + 24));
                //Заменить по элементам vieState и eventValidation все / на %2F,  + на %2B, = на %3D
                var vieStateEnc = encodeURIComponent(vieState);
                var vieStateGeneratorEnc = encodeURIComponent(vieStateGenerator);
                var eventValidationEnc = encodeURIComponent(eventValidation);
                //ID записи
                Index = FirstResponse.slice(FirstResponse.lastIndexOf('fnSelectGridItem(', FirstResponse.indexOf(login)) + 16, FirstResponse.lastIndexOf('fnSelectGridItem(', FirstResponse.indexOf(login)) + 20).match(/\d+/)[0];
                //__EVENTTARGET=OnlyActiveUsersCheckBox&__EVENTARGUMENT=&__LASTFOCUS=&__VIEWSTATE=%%2FSUQ9MWQWAmYPFQEY0J%2FQvtC70YzQt9C%2B0LLQsNGC0LXQu9C4ZAICD2QWAgIBDw8WAh8FBRhQcm9wZXJ0eUZpZWxkcy5hc3B4P0lEPThkFgJmDxUBJdCU0L7Qv9C%2B0LvQvdC40YLQtdC70YzQvdGL0LUg0L%2FQvtC70Y9kAgMPZBYCAgEPDxYCHwUFFEN1cnJlbmNpZXMuYXNweD9JRD0zZBYCZg8VAQzQktCw0LvRjtGC0YtkAgQPZBYCAgEPDxYCHwUFEUNoYXJnZXMuYXNweD9JRD00ZBYCZg8VARDQmtC%2B0LzQuNGB0YHQuNC4ZAIFD2QWAgIBDw8WAh8FBRdTY3JpcHRMaWJyYXJ5LmFzcHg%2FSUQ9NWQWAmYPFQEO0KHQutGA0LjQv9GC0YtkAgYPZBYCAgEPDxYCHwUFEkNsZWFyaW5nLmFzcHg%2FSUQ9NmQWAmYPFQEO0JrQu9C40YDQuNC90LNkAgcPZBYCAgEPDxYCHwUFG1RyYW5zYWN0aW9uUmVwb3J0LmFzcHg%%2B0LQfBQUYRGVmYXVsdC5hc3B4P0xvZ291dD10cnVlZGQCBw8PFgIfAQUY0J%2FQvtC70YzQt9C%2B0LLQsNGC0LXQu9C4ZGQCCQ9kFgICAQ8PFgIeB1Zpc2libGVoZBYCAgEPDxYCHwEFzAHQktCw0YjQuCDQv9GA0LDQstCwINC00LvRjyDRgNCw0LHQvtGC0Ysg0YEg0L%2FQvtC70YzQt9C%2B0LLQsNGC0LXQu9GP0LzQuCDQvtCz0YDQsNC90LjRh9C10L3Riy4g0KHQstGP0LbQuNGC0LXRgdGMINGBINGB0LjRgdGC0LXQvNC90YvQvCDQvNC10L3QtdC00LbQtdGA0L7QvCDQtNC70Y8g0YPQstC10LvQuNGH0LXQvdC40Y8g0JLQsNGI0LjRhSDQv9GA0LDQsi5kZAILDw8WBB8BBR8u0KLQntCSICLQn9C%2B0YHRgiDQpNGW0L3QsNC90YEiHwUFF09yZ2FuaXphdGlvbnMuYXNweD9JRD0xZGQCDQ8PFgQfAQU40KTRltC70ZbRjyDihJYxINCi0J7QkiAi0J%2FQvtGB0YIg0KTRltC90LDQvdGBIiwg0JrQuNGX0LIfBQUbQnJhbmNoZXMuYXNweD9JRD0xJnBJRD01NTU1ZGQCDw8PFgQfAQVA0J%2FQndCk0J8g4oSWMTAwMDEg0KTRltC70ZbRlyDihJYxINCi0J7QkiAi0J%2FQvtGB0YIg0KTRltC90LDQvdGBIh8FBSVBZ2VuY2llcy5hc3B4P0lEPTEmcElEPTU1NTUmcHBJRD01NTU2ZGQCEQ8PFgIfAQUY0J%2FQvtC70YzQt9C%2B0LLQsNGC0LXQu9C4ZGQCFQ8QDxYCHwEFNtCi0L7Qu9GM0LrQviDQsNC60YLQuNCy0L3Ri9C1INC%%2FDqMOxw7w%%2FQvdCwINCS0ZYuLi4fCwUJSUQ6IDEzNjUyZGQCAg9kFgICAQ8PFgIfAQUOYXNoY2hldWxvdmEueWFkZAIDD2QWAgIBDw8WAh8BBQoyMC4xMC4yMDE2ZGQCAw9kFghmD2QWAgIBDw9kFgIfCgV4amF2YXNjcmlwdDoge3dpbmRvdy5ldmVudC5jYW5jZWxCdWJibGUgPSB0cnVlOyAgcmV0dXJuIGNvbmZpcm0oJ8OCw7sgw7XDrsOyw6jDssOlIMOzw6TDoMOrw6jDssO8IMO9w7LDsyDDp8Ogw6%2FDqMOxw7w%2FJyl9ZAIBD2QWAgIBDw8WBB8BBR%%%2FDqMOxw7w%2FJyl9ZAIBD2QWAgIBDw8WBB8BBR%2FQk9C%%%%%2FDqMOxw7w%2FJyl9ZAIBD2QWAgIBDw8W