                    //AlreadyBlocked.push(CassiersAll[num1] + " " + CassiersAll[num1 + 1] + " " + CassiersAll[num1 + 2] + " " + CassiersAll[num1 + 3] + " " + CassiersAll[num1 + 4]);
                    // UndefinedPNFP = UndefinedPNFP + CassiersAll[num1] + "\t" + CassiersAll[num1 + 1] + " " + CassiersAll[num1 + 2] + " " + CassiersAll[num1 + 3] + "\t" + CassiersAll[num1 + 4] + "\n";
console.log("Если не найдены среди активных");
                }

            } else {
                //UndefinedPNFP.push(CassiersAll[num1] + " " + CassiersAll[num1 + 1] + " " + CassiersAll[num1 + 2] + " " + CassiersAll[num1 + 3] + " " + CassiersAll[num1 + 4]);
                UndefinedPNFP = UndefinedPNFP + CassiersAll[num1] + "\t" + CassiersAll[num1 + 1] + " " + CassiersAll[num1 + 2] + " " + CassiersAll[num1 + 3] + "\t" + CassiersAll[num1 + 4] + "\n";
            }
        }
    } else {
        alert("Недостаточно элементов в списке!");
    }
    if (DontFind !== "" || UndefinedPNFP !== "") {
        document.getElementsByName("Simbian")[0].value = "Логины не найдены!" + "\n" + DontFind + "\n" + "Учетки с ошибкой при поиске ПНФП!" + "\n" + UndefinedPNFP;

    } else {
        if (CassiersAll.length % 5 === 0) alert("Все кассиры зблокированы");
    }
}
  
