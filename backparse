                    xhr.open('POST', adress, false);
                    xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
                    xhr.send(params);
                    ThirdResponse = xhr.responseText;
                } else {
                    //ЕСЛИ ЛОГИН НЕ НАЙДЕН ИСЧЕМ В НЕАКТИВНЫХ
