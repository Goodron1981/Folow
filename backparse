document.getElementsByName("Simbian")[0].value = "ВСТАВЬТЕ СЮДА ДАННЫЕ УЧЕТНЫХ ЗАПИСЕЙ КОТОРЫЕ НЕОБХОДИМО ЗАБЛОКИРОВАТЬ!";
