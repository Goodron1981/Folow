				NeedChalk = SecondResponse.slice(SecondResponse.indexOf('Права пользователя'), SecondResponse.indexOf('UserRightsBehaviourPanel') );
                CheckBoxArray = NeedChalk.split('checked="checked"');
                subCBArray = '';
                for(l=0; l<CheckBoxArray.length-1;l++){
                   findedCB = CheckBoxArray[l].slice(CheckBoxArray[l].lastIndexOf('type="checkbox" name="')+22, CheckBoxArray[l].lastIndexOf('" ')); 
                   if(findedCB !== 'UserRightsList:_ctl0:RightCheckBox'){
                   subCBArray =subCBArray+encodeURIComponent(findedCB)+'=on&';
                   }
                }
                //Значение radio
                BehaviorValue = SecondResponse.slice(SecondResponse.lastIndexOf('UserRightBehaviour" value="',SecondResponse.lastIndexOf('checked="checked'))+27, SecondResponse.lastIndexOf('checked="checked')-2);
             
                //document.documentElement.innerHTML = SecondResponse;
                    //Второй запрос
                    //Берем значение нужных элементов для следующего POST запроса
                    vieState = SecondResponse.slice(SecondResponse.indexOf('VIEWSTATE" value=') + 18, SecondResponse.indexOf('"', SecondResponse.indexOf('VIEWSTATE" value=') + 18));
                    vieStateGenerator = SecondResponse.slice(SecondResponse.indexOf('VIEWSTATEGENERATOR" value=') + 27, SecondResponse.indexOf('"', SecondResponse.indexOf('VIEWSTATEGENERATOR" value=') + 27));
                    eventValidation = SecondResponse.slice(SecondResponse.indexOf('EVENTVALIDATION" value=') + 24, SecondResponse.indexOf('"', SecondResponse.indexOf('EVENTVALIDATION" value=') + 24));
                    //Заменить по элементам vieState и eventValidation все / на %2F,  + на %2B, = на %3D
                    vieStateEnc = encodeURIComponent(vieState);
                    vieStateGeneratorEnc = encodeURIComponent(vieStateGenerator);
                    eventValidationEnc = encodeURIComponent(eventValidation);
                    //Составляем параметры выставления блокировки учетной записи
                    params = "__EVENTTARGET=&__EVENTARGUMENT=&__LASTFOCUS=&__VIEWSTATE=" + vieStateEnc + "&__VIEWSTATEGENERATOR=" + vieStateGeneratorEnc + "&__EVENTVALIDATION=" + eventValidationEnc + "&OnlyActiveUsersCheckBox=on&UserNameTextBox=&LoginTextBox=&PasswordTextBox=&NewPasswordTextBox=&ConfirmPasswordTextBox=&" + subCBArray+'UserRightBehaviour='+ BehaviorValue +'&AddFieldsNamesCombo=&AddPropName=&AddPropValue=&UpdateUserButton=%D0%98%D0%B7%D0%BC%D0%B5%D0%BD%D0%B8%D1%82%D1%8C&UsersIndex=';
                    //Отправляем новый запрос POST
